{
  "expo": {
    "name": "tiktok-expo",
    "slug": "tiktok-expo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.tiktokexpo",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for live streaming",
        "NSMicrophoneUsageDescription": "This app uses the microphone for live streaming"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.tiktokexpo",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "We need access to your camera for live streaming",
          "microphonePermission": "We need access to your microphone for live streaming"
        }
      ],
      "expo-video",
      "expo-asset",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d7bbfc76-8691-4e49-a0ce-e7c257cea149"
      }
    },
    "owner": "tigerpanda"
  }
}
